<Window x:Class="Settings.StartWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Settings"
        mc:Ignorable="d"
        xmlns:tb="http://www.hardcodet.net/taskbar"        
        Title="StartWindow" Height="450" Width="800">
    <Grid>
       
        <tb:TaskbarIcon x:Name="myNotifyIcon"
                    Visibility="Visible"
                    ToolTipText="Open start window"
                    IconSource="strelka.ico"    
                     />
        <Button Content="Настройки" HorizontalAlignment="Left" Margin="323,179,0,0" VerticalAlignment="Top" Height="98" Width="214" Click="Button_Click"/>



    </Grid>
</Window>
